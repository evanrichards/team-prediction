{
  "name": "team-prediction",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "build:1-generate": "prisma generate",
    "build:2-migrate": "prisma migrate deploy",
    "build:3-next": "cross-env NODE_ENV=production next build",
    "build:4-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production node dist/server/prodServer.js",
    "studio": "prisma studio",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.3.8",
    "@trpc/client": "^10.6.0",
    "@trpc/next": "^10.6.0",
    "@trpc/react-query": "^10.6.0",
    "@trpc/server": "^10.6.0",
    "@vercel/analytics": "^0.1.6",
    "clsx": "^1.1.1",
    "flowbite": "^1.5.5",
    "flowbite-react": "^0.3.7",
    "next": "^13.1.1",
    "next-auth": "^4.18.7",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.0",
    "superjson": "^1.7.4",
    "tailwind-styled-components": "^2.2.0",
    "tsx": "^3.9.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.1",
    "@next/eslint-plugin-next": "^13.1.1",
    "@playwright/test": "^1.29.1",
    "@tanstack/react-query-devtools": "^4.3.8",
    "@types/node": "^18.7.20",
    "@types/node-fetch": "^2.5.11",
    "@types/react": "^18.0.9",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.8.1",
    "prettier-plugin-prisma": "^4.8.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.7.1",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.8.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
